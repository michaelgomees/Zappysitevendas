zappybotvendas
CREATE DATABASE zappybotvendas DEFAULT CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci;
CREATE USER 'wpuser'@'localhost' IDENTIFIED BY 'G?gUB6~QPLS*g';
GRANT ALL PRIVILEGES ON zappybotvendas.* TO 'wpuser'@'localhost';
FLUSH PRIVILEGES;
EXIT;
SHOW DATABASES LIKE 'zappybotvendas';
DROP DATABASE wordpress;
DROP USER 'zappyadmin'@'localhost';
FLUSH PRIVILEGES;
EXIT;
FLUSH PRIVILEGES;
ALTER USER 'root'@'localhost' IDENTIFIED BY '12345678';
EXIT;
USE mysql;
UPDATE user SET password=PASSWORD('12345678') WHERE User='root' AND Host='localhost';
FLUSH PRIVILEGES;
EXIT;
DROP USER 'root2'@'localhost';
FLUSH PRIVILEGES;
DROP DATABASE wordpress;
DROP USER 'zappyadmin'@'localhost';
FLUSH PRIVILEGES;
SHOW DATABASES;
USE zappybotvendas;
SHOW TABLES;
DROP DATABASE zappybotvendas;
